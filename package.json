{
  "name": "vscode-fsevents",
  "version": "1.2.11",
  "description": "Native Access to Mac OS-X FSEvents",
  "main": "fsevents.js",
  "os": [
    "darwin"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "node-gyp clean",
    "install": "[ -f fsevents.node ] || npm run prepare",
